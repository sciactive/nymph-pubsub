{
	"name": "sciactive/nymph-pubsub",
	"description": "A library for publishing/subscribing to Nymph database changes.",
	"version": "1.5.0",
	"time": "2017-06-16",
	"homepage": "http://nymph.io/",
	"type": "library",
	"authors": [
		{
			"name": "Hunter Perrin",
			"email": "hunter@sciactive.com",
			"homepage": "https://github.com/hperrin",
			"role": "Developer"
		}
	],
	"keywords": [
		"web sockets",
		"nymph",
		"pubsub",
		"publish",
		"subscribe"
	],
	"license": "Apache-2.0",
	"support": {
		"issues": "https://github.com/sciactive/nymph/issues",
		"wiki": "https://github.com/sciactive/nymph/wiki",
		"source": "https://github.com/sciactive/nymph-pubsub"
	},
	"archive": {
		"exclude": [
			"**/.*",
			"vendor"
		]
	},
	"autoload": {
		"psr-0": {
			"Nymph\\PubSub\\": "src/"
		},
		"psr-4": {
			"Nymph\\PubSub\\": "src/"
		}
	},
	"repositories": [
		{
			"type": "vcs",
			"url": "https://github.com/Devristo/phpws"
		}
	],
	"require": {
		"sciactive/nymph-server": "~1.5.0",
		"sciactive/hookphp": "~1.1",
		"sciactive/requirephp": "~1.3",
		"cboden/ratchet": "0.3.*",
    "zendframework/zend-log": "2.*",
		"textalk/websocket": "1.0.*"
	},
	"minimum-stability": "dev",
	"maybe-require-idk": {
		"php-amqplib/php-amqplib": "2.6.*"
	}
}
